Cell["Re-parameterizing Mars\[CloseCurlyQuote]s Position: From Polar to \
Cartesian", "Subsection",
 CellChangeTimes->{{3.758341121125433*^9, 
  3.758341173550495*^9}},ExpressionUUID->"82e9b860-eb2f-456d-8eb3-\
668152c693cd"],

Cell["\<\
In order to graph Mars\[CloseCurlyQuote]s position at each time point, we \
need to re-parameterize our position equation in terms of Cartesian \
coordinates.\
\>", "Text",
 CellChangeTimes->{{3.7583411813448057`*^9, 
  3.758341210437451*^9}},ExpressionUUID->"3446ffa7-217f-4663-b2fd-\
1ba4c502d510"],

Cell["X", "Item",
 CellChangeTimes->{{3.75834122382654*^9, 
  3.7583412268788013`*^9}},ExpressionUUID->"20a641c7-fc0f-4977-b7ab-\
9d6595ded5f7"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"u", "[", "t_", "]"}], ":=", 
   RowBox[{"First", "[", 
    RowBox[{"Evaluate", "[", 
     RowBox[{
      RowBox[{"First", "[", 
       RowBox[{"CoordinateTransform", "[", 
        RowBox[{
         RowBox[{"\"\<Polar\>\"", "\[Rule]", "\"\<Cartesian\>\""}], ",", 
         RowBox[{"{", 
          RowBox[{
           FractionBox[
            RowBox[{"e", "*", "d"}], 
            RowBox[{"1", "+", 
             RowBox[{"e", "*", 
              RowBox[{"Cos", "[", 
               RowBox[{"\[Theta]", "[", "t", "]"}], "]"}]}]}]], ",", 
           RowBox[{"\[Theta]", "[", "t", "]"}]}], "}"}]}], "]"}], "]"}], "/.",
       "s"}], "]"}], "]"}]}], "\n"}]], "Input",
 CellChangeTimes->{{3.757908123028461*^9, 3.757908139458672*^9}, 
   3.758333637186386*^9, {3.7583362266366568`*^9, 3.7583362324150877`*^9}, 
   3.7583412712971287`*^9},ExpressionUUID->"e9152c3e-be16-4c3a-9d5e-\
f0e7aac222d0"],

Cell["Y", "Item",
 CellChangeTimes->{{3.7583412432906437`*^9, 
  3.7583412439400387`*^9}},ExpressionUUID->"2c10ab27-c00a-4812-bfd1-\
135a7cd28b76"],

Cell[BoxData[
 RowBox[{
  RowBox[{"First", "[", 
   RowBox[{"Evaluate", "[", 
    RowBox[{
     RowBox[{"Last", "[", 
      RowBox[{"CoordinateTransform", "[", 
       RowBox[{
        RowBox[{"\"\<Polar\>\"", "\[Rule]", "\"\<Cartesian\>\""}], ",", 
        RowBox[{"{", 
         RowBox[{
          FractionBox[
           RowBox[{"e", "*", "d"}], 
           RowBox[{"1", "+", 
            RowBox[{"e", "*", 
             RowBox[{"Cos", "[", 
              RowBox[{"\[Theta]", "[", "t", "]"}], "]"}]}]}]], ",", 
          RowBox[{"\[Theta]", "[", "t", "]"}]}], "}"}]}], "]"}], "]"}], "/.", 
     "s"}], "]"}], "]"}], "\[IndentingNewLine]"}]], "Input",
 CellChangeTimes->{
  3.7583306101543903`*^9, {3.7583336509403*^9, 3.758333668785598*^9}, 
   3.758340930503797*^9, {3.758341263957078*^9, 
   3.758341274966338*^9}},ExpressionUUID->"3ca0a7b8-a8f3-4703-aa6f-\
c118e8eb8420"]
}, Open  ]],

